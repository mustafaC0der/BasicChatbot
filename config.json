{
    "api_endpoints": {
        "weather_api": "https://api.weather.com/v3/",
        "news_api": "https://newsapi.org/v2/",
        "joke_api": "https://official-joke-api.appspot.com/jokes/random",
        "fact_api": "https://some-fact-api.com/facts"
    },
    "api_keys": {
        "weather_api_key": "YOUR_WEATHER_API_KEY",
        "news_api_key": "YOUR_NEWS_API_KEY",
        "joke_api_key": "YOUR_JOKE_API_KEY",
        "fact_api_key": "YOUR_FACT_API_KEY"
    },
    "response_settings": {
        "use_voice_output": false,
        "voice_language": "en-US",
        "voice_speed": 1.0
    },
    "security": {
        "enable_encryption": true,
        "encryption_key": "YOUR_ENCRYPTION_KEY"
    },
    "timeout_settings": {
        "api_timeout": 5,
        "response_timeout": 3
    }
}
